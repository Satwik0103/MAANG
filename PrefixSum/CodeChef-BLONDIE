#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	  long sum=0;
	    for(int i=0;i<n;i++){
	        int x;
	        cin>>x;
	         if(x<0){
	         x=sum/i;
	        }
	        sum+=x;
	        cout<<x<<" ";
	    }
	    
	    
	    	cout<<endl;
	}



}
